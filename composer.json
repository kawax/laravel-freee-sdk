{
  "name": "revolution/laravel-freee-sdk",
  "description": "Freee SDK for Laravel",
  "keywords": [
    "freee",
    "freee-accounting",
    "laravel"
  ],
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2||^8.0",
    "ext-json": "*",
    "illuminate/support": "*",
    "freee/freee-accounting-sdk": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0||^9.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0||^5.0||^6.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Freee\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Revolution\\Freee\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Freee\\Providers\\FreeeServiceProvider"
      ]
    }
  }
}
